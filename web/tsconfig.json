{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["DOM", "ESNext", "ES2015"],
    "moduleResolution": "Node",
    "jsx": "preserve",
    "module": "ESNext",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "strict": true,
  }
}
