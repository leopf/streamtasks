{
  "compilerOptions": {
    "target": "ESNext",
    "incremental": true,
    "lib": ["ESNext", "dom", "dom.iterable"],
    "module": "CommonJS",
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react",
    "skipLibCheck": true,
    "experimentalDecorators": true
  }
}
